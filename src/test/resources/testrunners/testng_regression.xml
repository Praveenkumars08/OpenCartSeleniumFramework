<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Test Cases of OpenCart" thread-count="2" parallel="tests" verbose="4">
	<listeners>
		<listener class-name="com.opencart.listeners.ExtentReportListener" />
		<listener class-name="com.opencart.listeners.AllureReportListener" />
		<listener class-name="com.opencart.listeners.AnnotationTransformer" />
		
	</listeners>
	<test name="LoginPage Test">
		<classes>
			<class name="com.opencart.testcases.LoginPageTest" />
		</classes>
	</test>
	<test name="AccountsPage Test">
		<classes>
			<class name="com.opencart.testcases.AccountsPageTest" />
		</classes>
	</test>
</suite>